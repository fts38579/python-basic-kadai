      "source": [
        "# 課題：台形の面積を算出\n",
        "\n",
        "# 上辺(cm)\n",
        "upper = 10\n",
        "\n",
        "# 下辺(cm)\n",
        "lower = 20\n",
        "\n",
        "# 高さ(cm)\n",
        "height = 5\n",
        "\n",
        "# 台形の面積（cm²） = (上辺 + 下辺) × 高さ ÷ 2\n",
        "area = (upper + lower) * height / 2\n",
        "\n",
        "# 結果の値を単位『cm²』で出力\n",
        "print(f\"{area}cm²\")"
      ]
    }
  ]
